สี่เหลี่ยม
#include <graphics.h>
int main()
{
    initwindow(400,400);
    rectangle(100,100,300,300);
    getch();
    return 0;
}

วงกลม
#include <graphics.h>
int main()
{
    initwindow(400,400);
    circle(200,200,100);
    getch();
    return 0;
}

1/2วงกลม
#include <graphics.h>
int main()
{
    initwindow(400,400);
    arc(200,200,0,180,100);
    line(100,200,300,200);
    getch();
    return 0;
}

1/4วงกลม
#include <graphics.h>
int main()
{
    initwindow(400,400);
    arc(200,200,0,90,100);
    line(200,200,300,200);
    line(200,200,200,100);
    getch();
    return 0;
}

สามเหลี่ยม
#include <graphics.h>
int main()
{
    initwindow(400,400);
    line(200,100,100,300);
    line(200,100,300,300);
    line(100,300,300,300);
    getch();
    return 0;
}

ท่อ
#include <graphics.h>
int main()
{
    initwindow(400,400);
    circle(200,200,100);
    circle(200,200,70);
    getch();
    return 0;
}

กล่อง
#include <graphics.h>
int main()
{
    initwindow(400,400);
    rectangle(100,100,300,300);
    rectangle(150,150,250,250);
    getch();
    return 0;
}
CUT BEAM
#include <graphics.h>

int main()
{
    initwindow(400,400);


    setcolor(WHITE);
    rectangle(175,100,225,300);
    rectangle(100,300,300, 350);
    getch();
    closegraph();

    return 0;
}

H,I BEAM

#include <graphics.h>

int main()
{
    initwindow(400,400);


    setcolor(WHITE);
    rectangle(100,50,300,100);
    rectangle(180,100,220, 300);
    rectangle(100,300,300, 350);

    getch();
    closegraph();

    return 0;
}

Chanel

#include <graphics.h>

int main()
{
    initwindow(400,400);


    setcolor(WHITE);

    rectangle(100,50,300,100);
    rectangle(100,100,150, 300);
    rectangle(100,300,300, 350);


    getch();
    closegraph();

    return 0;
}

L BEAM


#include <graphics.h>

int main()
{
    initwindow(400,400);


    setcolor(WHITE);

    
    rectangle(100,100,150, 300);
    rectangle(100,300,300, 350);


    getch();
    closegraph();

    return 0;
}


